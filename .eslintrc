{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "quotes": [
      2,
      "double"
    ],
    "curly": 2,
    "semi": [
      2,
      "always"
    ],
    "no-console": [
      1,
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "complexity": [
      1,
      6
    ],
    "default-case": 2,
    "eqeqeq": [
      2,
      "always"
    ],
    "no-shadow": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false,
        "classes": true
      }
    ],
    "max-len": [
      2,
      {
        "code": 120,
        "ignoreStrings": true,
        "tabWidth": 4
      }
    ],
    "no-duplicate-imports": [
      2,
      {
        "includeExports": true
      }
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "no-multi-spaces": 2,
    "jsx-quotes": [
      2,
      "prefer-double"
    ],
    "space-in-parens": 1,
    "template-tag-spacing": [
      2,
      "always"
    ],
    "no-unused-vars": [
      2,
      {
        "ignoreRestSiblings": true
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120,
        "tabWidth": 4,
        "semi": true,
        "trailingComma": "none",
        "jsxBracketSameLine": true
      }
    ]
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "react-app"
  ],
  "plugins": [
    "react",
    "eslint-plugin-prettier"
  ],
  "globals": {
  }
}